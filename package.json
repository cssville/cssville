{
  "name": "cssville",
  "version": "0.2.1",
  "description": "",
  "main": "cssville.css",
  "style": "cssville.css",
  "files": [
    "LICENSE.md",
    "cssville.css",
    "cssville.bundle.js",
    "generator.bundle.js"
  ],
  "scripts": {
    "build": "webpack",
    "generate": "node src/cssville.bundle.js src/cssville.css",
    "copy-css": "copyfiles -u 1 src/cssville.css .",
    "copy-js": "copyfiles -u 1 src/cssville.bundle.js src/generator.bundle.js .",
    "copy": "npm-run-all copy-css copy-js",
    "watch:build": "webpack --watch",
    "watch:server": "lite-server -c lite-server-config.json",
    "start": "npm-run-all build generate --parallel watch:build watch:server --print-label",
    "deploy-prepare": "npm-run-all build generate copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cssville/cssville.git"
  },
  "keywords": [],
  "author": "elv1s42",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cssville/cssville/issues"
  },
  "homepage": "https://github.com/cssville/cssville#readme",
  "devDependencies": {
    "@types/highlight.js": "^10.1.0",
    "@types/node": "^16.6.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "copyfiles": "^2.4.1",
    "highlight.js": "^11.7.0",
    "highlightjs": "^9.16.2",
    "lite-server": "^2.6.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
